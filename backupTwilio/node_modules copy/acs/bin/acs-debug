#!/usr/bin/env node

var child_proc = require('child_process'),
    path = require('path');

var nettlePath = path.join(__dirname, '../lib/nettle.js');
var args = ['--harmony-proxies', '--debug', nettlePath];

if(process.argv.length > 2) {
    for(i = 2; i < process.argv.length; i++) {
        args.push(process.argv[i]);
    }
}

var nettle = child_proc.spawn(process.execPath, args, {stdio: 'inherit'});

nettle.on('exit', function (code) {
   process.exit(code);
});
